{
    "name": "ygavrilov/translations",
    "description": "Translations plugin for CakePHP to manage translations in database and generate .po files",
    "type": "cakephp-plugin",
    "license": "MIT",
    "version": "1",
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Translations\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Translations\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
